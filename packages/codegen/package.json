    "semver": "7.5.4",
    "swagger-parser": "10.0.3",
    "validator": "13.7.0",
    "yargs": "17.6.2",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@types/node": "22.3.0",
    "@types/yargs": "17.0.14",
